@model DiaryMVC.Models.ListViewModeModel

<div class="list-mode">
    <div class="list-header">See all notes below</div>
    <div class="list-content">
        @foreach (var note in Model.Notes)
        {
            <div class="list-note">
                @Html.Partial("Diary/Note", note)
            </div>          
        }
    </div>
</div>